
include "/home/tiago/.gtkrc-2.0.mine"
gtk-theme-name="OSX-Breeze"
gtk-icon-theme-name="revival-blue"
gtk-font-name="FantasqueSansMono 8"
gtk-cursor-theme-name="Black"
gtk-cursor-theme-size=0
gtk-toolbar-style=GTK_TOOLBAR_ICONS
gtk-toolbar-icon-size=GTK_ICON_SIZE_LARGE_TOOLBAR
gtk-button-images=0
gtk-menu-images=0
